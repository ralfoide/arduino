<cvbyte nr="1" cat="Decoder address" desc="Base Address (6 LSB of accessory decoder address)" info="Decoder address low byte in range of 1-63."/>
<cvbyte nr="7" cat="info" desc="Manufacturer Version No." readonly="true" info="manufacturer's version number.  (2.1.3 or 213"/>
<cvbyte nr="8" cat="info" desc="Manufacturer ID" readonly="true" info="NMRA assignedmanufacturer's id number. Rpi=13 (DIY and PD devices)"/>
<cvbyte nr="9" cat="address" desc="Base Address (High byte)" info="Decoder address high byte - range of 0-7."/>
<cvbyte nr="29" cat="config" desc="Configuration" readonly="true" info="Configuration data (128 for accessory decoder)."/>
